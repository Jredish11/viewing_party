<div id= "header" >
  <h3> <%= @user.name %>'s Dashboard</h3>
</div>

<div id= "viewing_party_list" >
  <h5>Viewing Parties</h5>
</div>

<%= button_to "Discover Movies", "/users/#{@user.id}/discover", method: :get  %><br>

<% @user.parties.each do |party| %>

