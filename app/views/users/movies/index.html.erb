<div style="text-align: center;">
  <%= button_to "Discover Page", user_discover_path(@user), method: :get %><br><br>
  
  <% @movies.each do |movie| %>
    <div id="movie-<%=movie.id%>">
      <%= link_to "#{movie.title}", user_movie_path(@user, movie.id), method: :get %>
      Vote Average: <%= movie.vote_average %><br>
    </div>
  <% end %>
</div>