<div style="text-align: center;">
  <div id="existing-users">
    <h1>Existing Users</h1>
    <% @users.each do |user| %>
      <p><%= link_to "#{user.name}", "/users/#{user.id}" %></p>
    <% end %>
  <div><br>

  <div id="create-user">
    <%= button_to "Create User", "/register", method: :get %>
  </div>
  
  <div id="log-in-link">
    <%= link_to "Log-in form", "/login", method: :get %>
  </div>
</div>
